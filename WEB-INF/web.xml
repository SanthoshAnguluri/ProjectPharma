<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0">
    <context-param>
        <param-name>cName1</param-name>
        <param-value>cvalue1</param-value>
    </context-param>
   

    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.start.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>homeServlet</servlet-name>
        <servlet-class>com.start.homeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homeServlet</servlet-name>
        <url-pattern>/homeServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>errLoginServlet</servlet-name>
        <servlet-class>com.start.errLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>errLoginServlet</servlet-name>
        <url-pattern>/errLoginServlet</url-pattern>
    </servlet-mapping>
